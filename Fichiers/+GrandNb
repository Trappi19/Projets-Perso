int l=[0, 4, 6, 8, 4, 6, 25, 52, 14, 45, 63, 20, 23]

public class NbMax
{
    public static void NbPlusGrand(list l)
    {
        int max = 0
        foreach (var i in l)
        {
            if (i > max)
            {
                max = i
            }
            Console.WriteLine(max);
        }
    }
}